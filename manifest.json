{
  "manifest_version": 3,
  "name": "Foulmouth Filter",
  "version": "0.1",
  "icons": {
    "48": "icon.png"
  },
  "description": "Filters out filthy words from document titles",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["filter.js"]
    }
  ],
  "permissions": [
    "activeTab", "storage", "scripting"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Accessability",
    "default_popup": "table.html"
  }
}
